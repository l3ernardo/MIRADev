{{#if editmode}}
{{!-- AUACCOUNT form - Administrative Use Only section - Edit Mode --}}
	<table id="admin_table" cellspacing="0" cellpadding="0" border="0" width="100%" class="ibm-data-table">
		<caption class="mira-caption"><em>For Administrative Use Only</em></caption>
		{{#if GTSflag}}
		{{#if admin}}
			<tr>
				<td width="25%">
				    <input type="text" id="MetricsCriteriaLabel" name="MetricsCriteriaLabel" value="{{MetricsCriteriaLabel}}" size="40"><label> ($M USD)</label>
                </td>
				<td>
					<input type="text" id="MetricsCriteria" name="MetricsCriteria" value="{{MetricsCriteria}}" size="100" maxlength="300">
				</td>
			</tr>
			<tr>
			    <td width="25%"><label for="SpecialContractCategory">Special Contract Category:</label></td>
			    <td><input type="text" id="SpecialContractCategory" name="SpecialContractCategory" value="{{SpecialContractCategory}}" size="100" maxlength="300"></td>
			</tr>
		{{else}}
			<tr><td width="25%">{{MetricsCriteriaLabel}} ($M USD)</td><td>{{MetricsCriteria}} (planning purposes only)</td></tr>
		    <tr><td width="25%">Special Contract Category:</td><td>{{SpecialContractCategory}}</td></tr>
		{{/if}}
		{{else}}
				{{#if admin}}
			<tr>
				<td width="25%"><input type="text" id="MetricsCriteriaLabel" name="MetricsCriteriaLabel" value="{{MetricsCriteriaLabel}}" size="40"><label> ($M USD)</label></td>
				<td>
					<input type="text" id="MetricsCriteria" name="MetricsCriteria" value="{{MetricsCriteria}}" size="100" maxlength="300">
				</td>
			</tr>
		{{else}}
			<tr><td width="25%">{{MetricsCriteriaLabel}} ($M USD)</td><td>{{MetricsCriteria}} (planning purposes only)</td></tr>
		{{/if}}
		{{/if}}
	</table>
{{else}}
{{!-- AUACCOUNT form - Administrative Use Only section - Read Mode --}}
	<table id="admin_table" cellspacing="0" cellpadding="0" border="0" width="100%" class="ibm-data-table">
		<caption class="mira-caption"><em>For Administrative Use Only</em></caption>
		{{#if GTSflag}}
		<tr><td width="25%">{{MetricsCriteriaLabel}} ($M USD)</td><td>{{MetricsCriteria}} (planning purposes only)</td></tr>
		<tr><td width="25%">Special Contract Category:</td><td>{{SpecialContractCategory}}</td></tr>
		{{else }}
		<tr><td width="25%">{{MetricsCriteriaLabel}} ($M USD)</td><td>{{MetricsCriteria}} (planning purposes only)</td></tr>
		{{/if}}
	</table>
{{/if}}
