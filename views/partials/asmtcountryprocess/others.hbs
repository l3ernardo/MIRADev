<table id="other_table" class="ibm-data-table">
	{{#if ShowEA}}
		<thead class="other">
			<th class="other">Environmental Affairs Assessment</th>
		</thead>
		<tr class="other">
			<td>
				<table width="100%" class="other-viewheader">
					<thead>
						<th class="other-viewheader" scope="col" width="35%">Question</th>
						<th class="other-viewheader" scope="col" width="10%">Response</th>
						<th class="other-viewheader" scope="col" width="10%">Rating</th>
						<th class="other-viewheader" scope="col" width="10%">Target to Sat</th>
						<th class="other-viewheader" scope="col" width="35%">Explanation</th>
					</thead>
					<tr>
						<td>Did you perform/complete an Enviromental Affairs Assessment?</td>
						<td class="content-data">{{EAResponse}}</td>
						<td class="content-data">{{{ratingDisplay EARating}}}</td>
						<td class="content-data">{{EATarget2Sat}}</td>
						<td class="content-data">{{EARatingExplanation}}</td>
					</tr>
				</table>
			</td>
		</tr>
		<tr class="other">
			<td>
				<table width="100%" cellspacing="0" cellpadding="0" border="0" summary="Data table with alternating column example" class="ibm-data-table ibm-sortable-table ibm-alternating">
					<thead class="other-viewheader">
						<th class="other-viewheader" width="10">Period</th>
						<th class="other-viewheader" width="20">Rating</th>
						<th class="other-viewheader" width="40">Assessed by</th>
						<th class="other-viewheader" width="20">Target to Sat</th>
					</thead>
					{{#each EAData}}
						<tr class="asmt-viewdata">
							{{#each col}}
									<td>{{#if_equal this ""}}&nbsp;{{else}}{{this}}{{/if_equal}}</td>
							{{/each}}
						</tr>
					{{/each}}
				</table>
			</td>
		</tr>
	{{/if}}
	<thead class="other">
		<th class="other">Other Considerations</th>
	</thead>
	<tr class="other">
		{{#if editmode}}
			<td class="content-data">
				<div class='yui-skin-sam'>
					<span><textarea id="AsmtOtherConsiderations" type="text" name="AsmtOtherConsiderations" cols="168" rows="3">{{AsmtOtherConsiderations}}</textarea></span>
				</div>
			</td>
		{{else}}
			<td class="content-data">&nbsp;
				<input type="hidden" name="AsmtOtherConsiderationsRO" value="{{AsmtOtherConsiderations}}">
				<div id="AsmtOtherConsiderationsReadOnly"></div>
			</td>
		{{/if}}
	</tr>
</table>
