<textarea name="textarea" id="PPRDataExport" cols="100" style="display:none">{{valuesToString exportPPR}}</textarea>
<textarea name="textarea" id="LocalAuditDataExport" cols="100" style="display:none">{{valuesToString exportLocalAuditsList}}</textarea>
<textarea name="textarea" id="InternalAuditDataExport" cols="100" style="display:none">{{valuesToString AuditTrustedRCUData}}</textarea>
<table id="auditreview_table" class="ibm-data-table">
	<tr class="audit-review-content">
		<a href="javascript:void(0);" value="Export to Excel" name="ppr-link-export" id="ppr-link-export" class="ibm-symp-spreadsheet">Export to Excel</a>
		<span class="ibm-sep"> </span>
		<a href="javascript:void(0);" value="Export to Open Office" name="ppr-link-export2" id="ppr-link-export2" class="ibm-symp-spreadsheet">Export to Open Office</a>
		<td>
			<span class="audit-review"><b>Internal Audits and Proactive Reviews (Process Reviews, PwC Required Walkthroughs, IBM Voluntary Walkthroughs, and Application Reviews)</b></span><br /><br />
			<table id="ppr_treeview" width="100%" class="table_with_scroll">
				<thead>
					<!--<th class="audit-review-viewheader" width="2%"><input type="checkbox" class="ppr_checkbox_tree" id="ppr_checkbox_tree" name="ppr_checkbox "/></th>-->
					<th class="audit-review-viewheader" width="6%">Period</th>
					<th class="audit-review-viewheader" width="3%">CO</th>
					<th class="audit-review-viewheader" width="15%">Audit or Review</th>
					<th class="audit-review-viewheader" width="10%">ID</th>
					<th class="audit-review-viewheader" width="20%">Assessable Unit</th>
					<th class="audit-review-viewheader" width="5%">Date</th>
					<th class="audit-review-viewheader" width="5%">Rating</th>
					<th class="audit-review-viewheader" width="3%">Total Recs</th>
					<th class="audit-review-viewheader" width="3%">Open Recs</th>
					<th class="audit-review-viewheader" width="7%">Target to Close</th>
					<th class="audit-review-viewheader" width="20%">Comments</th>
				</thead>
				<tbody>
				{{#each AuditTrustedData}}
				<tr data-tt-id={{id}} data-tt-parent-id="{{parent}}" class="asmt-viewdata">
					<!--<th width="2%">{{#if countryProcess}}<input type="checkbox" class="ppr_checkbox_tree"/>{{/if}}</th>-->
						<td width="6%">{{#unless countryProcess}}{{reportingQuarter}}{{/unless}}</td>
						<td width="3%">{{#if COFlag}}<a class="ibm-check-link"></a>{{/if}}</td>
						<td width="15%">{{auditOrReview}}</td>
						<td width="10%">{{#unless catEntry}}<a target="_blank" href="/asmtcomponents?id={{_id}}">{{eid}}</a>{{/unless}}</td>
						<td width="20%">{{countryProcess}}</td>
						<td width="5%">{{reportDate}}</td>
						<td width="5%">{{rating}}</td>
						<td width="3%">{{numRecommendationsTotal}}</td>
						<td width="3%">{{numRecommendationsOpen}}</td>
						<td width="7%">{{targetClose}}</td>
						<td width="20%">{{comments}}</td>
					</tr>
				{{/each}}
			</tbody>
			</table>
		</td>
	</tr>
	<tr class="audit-review-content">
		<td>
			<span class="audit-review"><b>Relevant CU Internal Audits</b></span><br /><br />
			<a href="javascript:void(0);" value="Export to Excel" name="InternalAudit-link-export" id="InternalAudit-link-export" class="ibm-symp-spreadsheet">Export to Excel</a>
			<span class="ibm-sep"> </span>
			<a href="javascript:void(0);" value="Export to Open Office" name="InternalAudit-link-export2" id="InternalAudit-link-export2" class="ibm-symp-spreadsheet">Export to Open Office</a>
			<table id="InternalAudit_table" width="100%" class="table_with_scroll">
				<thead>
					<th class="audit-review-viewheader" width="6%">Period</th>
					<th class="audit-review-viewheader" width="15%">Audit or Review</th>
					<th class="audit-review-viewheader" width="10%">ID</th>
					<th class="audit-review-viewheader" width="22%">Assessable Unit</th>
					<th class="audit-review-viewheader" width="5%">Report Date</th>
					<th class="audit-review-viewheader" width="5%">Rating</th>
					<th class="audit-review-viewheader" width="5%">Total Recs</th>
					<th class="audit-review-viewheader" width="5%">Open Recs</th>
					<th class="audit-review-viewheader" width="7%">Target to Close</th>
					<th class="audit-review-viewheader" width="20%">Comments</th>
				</thead>
				<tbody>
				{{#each AuditTrustedRCUData}}
					<tr class="asmt-viewdata">
						{{#if RPTG_QTR}}
							{{#if_equal RPTG_QTR ""}}
								<td width="6%">&nbsp;</td>
							{{else}}
								<td width="6%">{{RPTG_QTR}}</td>
							{{/if_equal}}
						{{else}}
							<td width="6%">&nbsp;</td>
						{{/if}}
						<td width="15%"><a target="_blank" href="/asmtcomponents?id={{_id}}">{{REVIEW_TYPE}}</a></td>
						<td width="10%">{{REVIEW_ID}}</td>
						<td width="22%">{{countryProcess}}</td>
						<td width="5%">{{REVIEW_END_DATE}}</td>
						<td width="5%">{{RATING}}</td>
						<td width="5%">{{NUM_TOTAL_RECMNDTNS}}</td>
						<td width="5%">{{NUM_OPEN_RECMNDTNS}}</td>
						<td width="7%">{{REVIEW_TARGET_CLOSURE_DATE}}</td>
						<td width="20%">{{Comments}}</td>
					</tr>
				{{/each}}
			</tbody>
			</table>
		</td>
	</tr>
	<tr class="audit-review-content">
		<td>
			<span class="audit-review"><b>Audits &amp; Reviews (data managed locally)</b></span><br /><br />
			<a href="javascript:void(0);" value="Export to Excel" name="LocalAudit-link-export" id="LocalAudit-link-export" class="ibm-symp-spreadsheet">Export to Excel</a>
			<span class="ibm-sep"> </span>
			<a href="javascript:void(0);" value="Export to Open Office" name="LocalAudit-link-export2" id="LocalAudit-link-export2" class="ibm-symp-spreadsheet">Export to Open Office</a>
			<table id="LocalAudit_treeview" width="100%" class="table_with_scroll">
			<!--{{#if admin}}-->
			<!--{{/if}}-->
				<a target="_blank" href="/localaudit?id={{_id}}&new" class="ibm-add1-link">New</a></td>

				<br /><br />

				<thead>
					<th class="audit-review-viewheader" width="7%">Period</th>
					<th class="audit-review-viewheader" width="20%">Audit or Review</th>
					<th class="audit-review-viewheader" width="7%">Date</th>
					<th class="audit-review-viewheader" width="7%">Rating</th>
					<th class="audit-review-viewheader" width="10%">Total Recs</th>
					<th class="audit-review-viewheader" width="9%">Open Recs</th>
					<th class="audit-review-viewheader" width="10%">Target to Close</th>
					<th class="audit-review-viewheader" width="30%">Comments</th>
				</thead>
				<tbody>
				{{#each AuditLocalData}}
					<tr data-tt-id={{id}} data-tt-parent-id="{{parent}}" class="asmt-viewdata">
						{{#unless parent}}
							<td width="6%">{{reportingQuarter}}</td>
						{{else}}
							<td width="6%">&nbsp;</td>
						{{/unless}}
						<td width="20%"><a target="_blank" href="/asmtcomponents?id={{id}}">{{auditOrReview}}</a></td>
						<td width="7%">{{reportDate}}</td>
						<td width="7%">{{rating}}</td>
						<td width="10%">{{numRecommendationsTotal}}</td>
						<td width="9%">{{numRecommendationsOpen}}</td>
						<td width="10%">{{targetCloseOriginal}}</td>
						<td width="30%">{{comments}}</td>
					</tr>
				{{/each}}
			</tbody>
			</table>
		</td>
	</tr>
</table>
