  					<th class="audit-review-viewheader" scope="col" width="5%">Rating</th>
  					<th class="audit-review-viewheader" scope="col" width="7%">Size</th>
  					<th class="audit-review-viewheader" scope="col" width="5%">Score</th>
  				</thead>
  				{{#each exportInternalAuditData}}
  					<tr data-tt-id={{id}} data-tt-parent-id="{{parentid}}" class="asmt-viewdata">
              <td width="10%">{{plannedStartDate}}</td>
              <td width="8%">{{engagement}}</td>
              <td width="40%"><a target="_blank" href="/asmtcomponents?id={{id}}&pid={{parentid}}">{{Name}}</a></td>
              <td width="20%">{{DocSubType}}</td>
              {{{ratingDisplayView PeriodRatingPrev 5}}}
              {{{ratingDisplayView PeriodRating 5}}}
              <td width="7%">{{CUSize}}</td>
              <td width="5%">{{CUScore}}</td>
  					</tr>
  				{{/each}}
          <tr class="audit-review-viewheader-ar">
            <td class="audit-review-viewheader-ar" colspan="9"><b>Weighted Score: {{WeightedAuditScore}}</b></td>
          </tr>
  			</table>
        <table width="100%" class="ibm-data-table">
					<tr>
						<td class="audit-review-viewheader-al" width="10%"><b>Explanations</b></td>
						<td class="asmt-viewdata" width="90%"><input type="text" name="IAExplanations" id="IAExplanations" value="{{IAExplanations}}" size="167"></td>
					</tr>
				</table>
  		</td>
  	</tr>
