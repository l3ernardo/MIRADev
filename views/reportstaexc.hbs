<link href="/public/css/jquery.treetable.css" rel="stylesheet" type="text/css" />
<link href="/public/css/style.css" rel="stylesheet" type="text/css" />
<script src='./js/report.js'></script>
<script src='./js/utility.js'></script>
<link href="./css/jquery.dataTables.css" rel="stylesheet" type="text/css">
<script src="./lib/jquery.dataTables.js"></script>
<div class="ibm-container" style="width:100%">
	<div class="ibm-container-body">
		<span><br/></span>
		<div id="eventLinks" name="eventLinks" class="ibm-buttons-row">
			<p>
				<span class="ibm-sep"> </span>
				<a href="javascript:void(0);" value="Export to Excel" name="lnk_exportxls" id="lnk_exportxls" class="ibm-symp-spreadsheet">Export to Excel</a>
				<span class="ibm-sep"> </span>
				<a href="javascript:void(0);" value="Export to Open Office" name="lnk_exportods" id="lnk_exportods" class="ibm-symp-spreadsheet">Export to Open Office</a>
				<textarea name="textarea" id="dataForExport" cols="100" style="display:none">{{valuesToString exportInfo}}</textarea>
			</p>
			<br/>
		</div>
		<div>
			<!--<table id="reports_treeview1" cellspacing="0" cellpadding="0" border="0" style="width:100%"  class="display nowrap" class="mira-dashboard-table">-->
			<table id="reports_treeview1" class="mira-dashboard-table display" cellspacing="0" cellpadding="0" border="0">
				<thead>
					<tr>
						<!--<th style="width:1%;" scope="col"><input type="checkbox" class="mira_checkbox_tree" id="mira_checkbox_tree" name="mira_checkbox "/></th>-->
						<th style="width:10%;" scope="col"><input type="checkbox" class="mira_checkbox_tree" id="mira_checkbox_tree" name="mira_checkbox "/></th>
						<th style="width:10%;" scope="col">BU IOT</th>
						<th style="width:10%;" scope="col">Type</th>
						<th style="width:10%;" scope="col">Assessable Unit</th>
						<th style="width:10%;" scope="col">MIRA</th>
						<th style="width:10%;" scope="col">WWBCIT</th>
						<th style="width:10%;" scope="col">CurrentQ</th>
					</tr>
				</thead>
				<tbody>
				{{#each doc}}
					<tr data-tt-id={{id}} data-tt-parent-id={{parent}}>
						<!--<td style="width:1%;" nowrap><input type="checkbox" class="mira_checkbox_tree"/></td>-->
					    <td style="width:10%;" nowrap>{{#unless catEntry}}<input type="checkbox" class="mira_checkbox_tree"/>{{/unless}} {{#if catEntry}}{{GroupingName}}{{/if}}</td>
						<td style="width:10%;" nowrap>{{IOTName}}</td>
						{{#if cutype}}
							<td style="width:10%;">{{cutype}}</td>
						{{else}}
							<td style="width:10%;">{{DocSubType}}</td>
						{{/if}}
						<td style="width:10%;"><a href="/assessableunit?id={{_id}}">{{Name}}</a></td>
						<td style="width:10%;">{{MIRAAssessmentStatus}}</td>
						<td style="width:10%;">{{WWBCITAssessmentStatus}}</td>
						<td style="width:10%;">{{PeriodRating}}</td>
					</tr>
				{{/each}}
				</tbody>
			</table>
		</div>
	</div>
</div>
<script>
	//Load document's name
	$('h1#pageTitle').text("Status Exceptions");
</script>
