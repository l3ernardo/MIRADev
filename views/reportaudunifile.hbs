<link href="/public/css/jquery.treetable.css" rel="stylesheet" type="text/css" />
<link href="/public/css/style.css" rel="stylesheet" type="text/css" />
<script src='./js/report.js'></script>
<script src='./js/utility.js'></script>
<link href="./css/jquery.dataTables.css" rel="stylesheet" type="text/css">
<script src="./lib/jquery.dataTables.js"></script>
<script>
	//Load document's name
	$('h1#pageTitle').text("Auditable Unit Files");
</script>
<div class="ibm-container" style="width:100%">
	<div class="ibm-container-body">
		<span><br/></span>
		<div id="eventLinks" name="eventLinks" class="ibm-buttons-row">
			<p>
				<span class="ibm-sep"> </span>
				<a href="javascript:void(0);" value="Export to Excel" name="lnk_exportxls" id="lnk_exportxls" class="ibm-symp-spreadsheet">Export to Excel</a>
				<span class="ibm-sep"> </span>
				<a href="javascript:void(0);" value="Export to Open Office" name="lnk_exportods" id="lnk_exportods" class="ibm-symp-spreadsheet">Export to Open Office</a>
				<textarea name="textarea" id="dataForExport" cols="100" style="display:none">{{valuesToString exportInfo}}</textarea>
			</p>
			<br/>
		</div>
		<div>
			<table id="reports_treeview2" cellspacing="0" cellpadding="0" border="0" style="width:100%"  class="display nowrap" class="mira-dashboard-table">
				<thead>
					<tr>
						<th style="width:1%;" scope="col"><input type="checkbox" class="mira_checkbox_tree" id="mira_checkbox_tree" name="mira_checkbox "/></th>
					    <th scope="col">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
						<th scope="col">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
					    <th >Auditable Unit Name&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
						<th scope="col" FF0808>Count</th>
						<th scope="col">Type&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</th>
						<th scope="col">Auditable Process</th>
						<th scope="col">Audit Readiness Review Status</th>
						<th scope="col">CQ - 4</th>
						<th scope="col">CQ - 3</th>
						<th scope="col">CQ - 2</th>
						<th scope="col">CQ - 1</th>
						<th scope="col">Current Quarter (CQ)</th>
						<th scope="col">Outlook</th>
						<th scope="col">Target to Sat</th>
						<th scope="col">Size</th>
						<th scope="col">Max Score</th>
						<th scope="col">CU Score</th>
						<th scope="col">Explanations</th>
					</tr>
				</thead>
				<tbody>
				{{#each doc}}
					    <tr data-tt-id={{_id}} data-tt-parent-id={{parentidauf}}>
								<td><input type="checkbox" class="mira_checkbox_tree"/></td>
							<th><font COLOR="red">{{Total}}</font></th>
						<th><font COLOR="red">{{CategoryName}}</font></th>
						<td>{{{in_Count Name _id Name}}}</td>
						<td>{{Count}}</td>
						<td>{{{in_Type DocSubType DocSubType}}}</td>
					    <td>{{AuditableProcess}}</td>
                        <td>{{AuditReadiness}}</td>
                        <td>{{CQ4}}</td>
						<td>{{CQ3}}</td>
                        <td>{{CQ2}}</td>
                        <td>{{CQ1}}</td>
						<td>{{AUNextQtrRating}}</td>
						<td>{{Target2Sat}}</td>
                        <td></td>
						<td>{{MIRAAssessmentStatus}}</td>
						<td>{{WWBCITAssessmentStatus}}</td>
						<td>{{PeriodRatingPrev}}</td>
						<td>{{PeriodRating}}</td>
					</tr>
				{{/each}}
				</tbody>
			</table>
		</div>
	</div>
</div>
